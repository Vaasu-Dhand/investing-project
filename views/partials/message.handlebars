{{#if errors}}
{{#each errors}}
{{!-- <li>{{this}}</li> --}}
<div class="alert alert-secondary alert-dismissible fade show" role="alert">
  {{this}}
  <button type="button" class="close" data-dismiss="alert" aria-label="Close">
    <span aria-hidden="true">&times;</span>
  </button> 
</div>
{{/each}}
{{/if}}